{% extends 'base.html.twig' %}
{% block title%} List des terrain {% endblock %}

{% block body %}
    {% if  f %}
<table  id = " f "  class = " table table-striped ">



    <table class="table">
        <thead>
        <tr>

            <th scope="col">code</th>
            <th scope="col">name</th>
            <th scope="col">address</th>
            <th scope="col">space</th>


        </tr>
        </thead>
        <tbody>
        {% for details in f %}
        <tr>
            <td>{{ f.id }}</td>
            <td>{{ f.price }}</td>
            <td>{{ f.address }}</td>
            <td>{{ f.space }}</td>




        </tr>
        {% endfor %}
        </tbody>
    </table>
    {% endif %}

{% endblock %}